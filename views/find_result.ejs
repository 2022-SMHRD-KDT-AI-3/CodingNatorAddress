  <style>
    #logo {
      height: 45px;
    }

    body>header,
    footer {
      background-color: #093023;
    }

    body {
      background-color: rgb(238, 238, 238);
    }

    main {
      background-color: white;
    }

    @media (max-width: 1068px) {
      ul.nav.col-12 {
        display: none;
      }

      div.col-md-3.text-end {
        flex-grow: 1;
      }

      #dropdownMenuButton1_h {
        display: inline-block !important;
      }
    }

    .dropdown-item:active {
      background-color: grey;
    }

    @media (max-width: 768px) {
      ul.nav.col-12 {
        display: none;
      }

      div.col-md-3.text-end {
        flex-grow: 1;
      }

      button#dropdownMenuButton1.dropdown-toggle::after {
        display: none;
      }
      button#dropdownMenuButton1_h.dropdown-toggle::after{
          display: none;
        }
      #dropdownMenuButton1_h {
        display: inline-block !important;
      }

    }

    #dropdownMenuButton1_h:hover path {
      fill: #000 !important;
    }

    #dropdownMenuButton1_h {
      display: none;
    }
  </style>
</head>

